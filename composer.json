{
    "name": "xyrotech/orin",
    "description": "Discog PHP Client",
    "keywords": [
        "xyrotech",
        "orin"
    ],
    "homepage": "https://github.com/xyrotech/orin",
    "license": "MIT",
    "authors": [
        {
            "name": "Adekunle Adelakun",
            "email": "kunle@xyrotech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "spatie/guzzle-rate-limiter-middleware": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Xyrotech\\Orin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xyrotech\\Orin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm --alter --issues=MissingReturnType,InvalidNullableReturnType",
        "test": "XDEBUG_MODE=coverage vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
